    <div class="container">

      <!-- Main hero unit for a primary marketing message or call to action -->
      <div class="hero-unit">
        <h1>Listing articles</h1>
          <table>
            <tr>
              <th>Time</th>
              <th>Category</th>
              <th>Subject</th>
              <th>Content</th>
              <th></th>
              <th></th>
              <th></th>
            </tr>
          <% @articles.each do |article| %>
            <tr>
              <td><%= article.time %></td>
              <td><%= article.category %></td>
              <td><%= article.subject %></td>
              <td><%= article.content %></td>
              <td><%= link_to 'Show', article, :class => 'btn' %></td>
              <td><%= link_to 'Edit', edit_article_path(article), :class => 'btn btn-primary btn-large'%></td>
              <td><%= link_to 'Destroy', article, confirm: 'Are you sure?', method: :delete, :class => 'btn'  %></td>
            </tr>
          <% end %>
          </table>
          <br />
          <%= link_to 'New Article', new_article_path %>
         </div>

